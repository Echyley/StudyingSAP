<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean
		class="de.hybris.platform.gigya.gigyab2bwebservices.dto.GigyaWebHookResponseWSDTO">
		<description>Gigya Webservices Response</description>
		<hints>
			<hint name="wsRelated" />
			<hint name="alias">GigyaWebHookResponseWSDTO</hint>
		</hints>
		<property name="code" type="int">
			<description>HTTP Response code, Usually returns 200 / 400</description>
			<hints>
				<hint name="example">200</hint>
			</hints>
		</property>
		<property name="result" type="java.lang.String">
			<description>Response result string, success / error</description>
			<hints>
				<hint name="example">success</hint>
			</hints>
		</property>
		<property name="message" type="java.lang.String">
			<description>Response message</description>
			<hints>
				<hint name="example">Gigya Webhook Event(s) received successfully.</hint>
			</hints>
		</property>
	</bean>
</beans>
