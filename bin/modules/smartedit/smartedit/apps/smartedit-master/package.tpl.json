{
    "version": "0.0.1",
    "private": true,
    "name": "smarteditmaster",
    "description": "Master SmartEdit application",
    "scripts": {
        "build": "npm run build:prod",
        "build:dev": "npm run build-container:dev && npm run build-inner:dev",
        "build:prod": "npm run build-container:prod && npm run build-inner:prod",
        "build-inner:dev": "npm run build-inner-only:dev && npm run copy-to-webroot -- --inner",
        "build-inner:prod": "npm run build-inner-only:prod && npm run copy-to-webroot -- --inner",
        "build-container:dev": "npm run build-container-only:dev && npm run copy-to-webroot -- --container",
        "build-container:prod": "npm run build-container-only:prod && npm run copy-to-webroot -- --container",
        "build-inner-only:dev": "webpack --config=webpack/inner/webpack.dev.js",
        "build-inner-only:prod": "webpack --config=webpack/inner/webpack.prod.js",
        "build-container-only:dev": "webpack --config=webpack/container/webpack.dev.js",
        "build-container-only:prod": "webpack --config=webpack/container/webpack.prod.js",
        "copy-to-webroot": "node ./scripts/copy-to-webroot.js",
        "lint": "npm run lint:base -- 'src/**/*.ts'",
        "lint:base": "eslint"
    },
    "devDependencies": {
        "@types/lodash": "4.14.189",
        "@types/node": "18.11.9",
        "css-loader": "6.7.1",
        "fs-extra": "10.1.0",
        "mini-css-extract-plugin": "2.6.1",
        "css-minimizer-webpack-plugin": "5.0.0",
        "source-map-loader": "4.0.0",
        "style-loader": "3.3.1",
        "terser-webpack-plugin": "5.3.3",
        "buffer": "6.0.3",
        "stream-browserify": "3.0.0",
        "ts-loader": "9.3.1",
        "eslint": "8.28.0",
        "@typescript-eslint/eslint-plugin": "5.45.0",
        "@typescript-eslint/parser": "5.45.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.6.4",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-deprecation": "1.3.3",
        "eslint-plugin-ban": "1.6.0",
        "eslint-plugin-prettier": "4.2.1",
        "typescript": "4.1.5",
        "webpack": "5.79.0",
        "webpack-cli": "5.0.1",
        "webpack-merge": "5.8.0",
        "fork-ts-checker-webpack-plugin": "7.2.13",
        "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
        "html-webpack-plugin": "5.5.0"
    },
    "dependencies": {
        "@webcomponents/webcomponentsjs": "2.7.0",
        "ckeditor4": "4.21.0",
        "core-js": "3.23.5",
        "jquery": "3.6.4",
        "lodash": "4.17.21",
        "@juggle/resize-observer": "3.4.0",
        "intersection-observer": "0.12.2",
        "whatwg-fetch": "3.6.2",
        "zone.js": "0.11.3",
        "@smartedit/scripts": "0.0.1",
        "smartedit-build": "0.0.1"
    }
}
